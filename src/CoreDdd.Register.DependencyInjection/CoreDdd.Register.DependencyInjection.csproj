<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net461;netstandard2.0</TargetFrameworks>
    <Description>ASP.NET Core Dependency Injection IoC registration for CoreDdd library</Description>
    <PackageTags>CoreDdd aspnetcore Dependency Injection ServiceProvider IoC</PackageTags>
  </PropertyGroup>

  <Import Project="..\CommonNugetAssemblyInfo.props" />
  <Import Project="..\SharedAssemblyInfo.props" />

  <PropertyGroup>
    <Version>4.0.0</Version>
    <PackageReleaseNotes>#51 Cumbersome casting of the query results (TResult)(object) in the query handlers - adding TReturn generic parameter into the IQuery interface to define the query return type</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.1.1" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CoreDdd\CoreDdd.csproj" />
  </ItemGroup>

</Project>
